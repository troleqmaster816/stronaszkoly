{
  "name": "timetable-viewer",
  "private": true,
  "version": "0.0.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "server": "node server/server.js",
    "dev:full": "concurrently -k -n server,vite -c blue,magenta \"npm:server\" \"npm:dev\"",
    "build": "tsc -b && vite build",
    "start": "NODE_ENV=production node server/server.js",
    "lint": "eslint .",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ui": "playwright test --ui",
    "preview": "vite preview",
    "docs:ui": "node server/server.js",
    "docs:lint": "spectral lint public/openapi.v1.draft.yaml"
  },
  "dependencies": {
    "@fontsource/space-grotesk": "^5.2.8",
    "@playwright/mcp": "^0.0.68",
    "compression": "^1.7.5",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dompurify": "^3.2.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.4.0",
    "framer-motion": "^12.23.12",
    "helmet": "^7.2.0",
    "js-yaml": "^4.1.0",
    "lucide-react": "^0.541.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.8.1",
    "swagger-ui-express": "^5.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@stoplight/spectral-cli": "^6.11.1",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.1.2",
    "eslint": "^9.32.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "playwright": "^1.55.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.0",
    "vite": "^7.1.0"
  }
}
